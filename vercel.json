{
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
      {
        "src": "/(.*)",
        "dest": "/"
      }
  ],
  "headers": [
      { "source": "/helper/whatsapp.js", "headers": [{ "key": "Cache-Control", "value": "no-store, no-cache, must-revalidate, max-age=0" }] }
  ]
}
  